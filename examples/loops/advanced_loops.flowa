# Advanced Loops - Nested and Complex Patterns

print("Advanced Loops Example")
print("============================================================")

# Nested loops - multiplication table
print("\n[1] Multiplication Table (3x3):")
i = 1
while i <= 3 {
    j = 1
    while j <= 3 {
        product = i * j
        print("  ", i, "x", j, "=", product)
        j = j + 1
    }
    i = i + 1
}

# Nested for loops - matrix operations
print("\n[2] Matrix sum:")
matrix = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
total = 0
for row in matrix {
    for value in row {
        total = total + value
    }
}
print("  Total sum:", total)

# Complex pattern - prime-like filtering
