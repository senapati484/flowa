# Nested loops with break/continue
print("\n[4] Find pairs that sum to 10:")
nums = [1, 2, 3, 4, 5, 6, 7, 8, 9]
found_count = 0
for a in nums {
    for b in nums {
        if a + b == 10 {
            if a < b {
                print("  Pair:", a, "+", b, "= 10")
                found_count = found_count + 1
            }
        }
    }
}
print("  Found", found_count, "pairs")

# While with complex condition
print("\n[5] Fibonacci sequence (first 10):")
a = 0
b = 1
count = 0
while count < 10 {
    print("  ", a)
    temp = a + b
    a = b
    b = temp
    count = count + 1
}

