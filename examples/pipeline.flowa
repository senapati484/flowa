# Data transformation pipeline example

def square(x):
    return x * x

def is_even(x):
    return x - (x / 2) * 2

def increment(x):
    return x + 1

# Pipeline composition
def pipeline(n):
    return n |> increment() |> square()

# Process data and print results
result1 = 5 |> pipeline()
result2 = 10 |> square() |> increment()

print(result1)
print(result2)
# Expected output:
# 36
# 101
