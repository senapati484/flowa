print("Starting intensive benchmark...")

run = 0
while run < 5:
    start = time.now_ms()  # define start here

    sum = 0
    i = 0
    
    while i < 10000000:
        sum = sum + i
        i = i + 1

    # Use time.since_s() instead - it returns a formatted string with decimal precision
    elapsed_s = time.since_s(start, 3)  # 3 decimal places

    print("Run:", run + 1, "Sum:", sum, "Time:", elapsed_s, "s")

    run = run + 1

print("Benchmark complete!")